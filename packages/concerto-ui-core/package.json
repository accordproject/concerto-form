{
  "name": "@accordproject/concerto-ui-core",
  "version": "0.71.3",
  "dependencies": {
    "composer-concerto": "^0.71.3"
  },
  "scripts": {
    "build": "rm -rf dist && NODE_ENV=production babel src --out-dir dist --ignore '**/*.spec.js,**/*.test.js,samples,__snapshots__,**/*.stories.js'",
    "watch": "npm run build -- --watch",
    "test": "jest --coverage --verbose --color",
    "pretest": "npm run licchk",
    "licchk": "license-check-and-add",
    "prepublishOnly": "npm run build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "@babel/cli": "7.4.3",
    "@babel/core": "7.4.3",
    "babel-jest": "24.7.1",
    "jest": "24.7.1",
    "license-check-and-add": "2.3.1"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "jest": {
    "setupFiles": [
      "../../setupTests"
    ]
  },
  "license-check-and-add-config": {
    "folder": ".",
    "license": "LICENSE.txt",
    "exact_paths_method": "EXCLUDE",
    "exact_paths": [
      "coverage",
      "LICENSE.txt",
      "node_modules",
      ".nyc-output",
      "out",
      ".git",
      "yarn.lock",
      "yarn-error.log",
      "README.md",
      ".babelrc",
      ".gitignore",
      "dist",
      "public"
    ],
    "file_type_method": "EXCLUDE",
    "file_types": [
      ".yml",
      ".yaml",
      ".zip",
      ".tgz"
    ],
    "license_formats": {
      "js|njk|pegjs|cto|acl|qry": {
        "prepend": "/*",
        "append": " */",
        "eachLine": {
          "prepend": " * "
        }
      },
      "npmrc|editorconfig|txt": {
        "eachLine": {
          "prepend": "# "
        }
      }
    }
  },
  "gitHead": "bdfdfb0a28e097d30ddb90ddcc5d2715a36f080b"
}
