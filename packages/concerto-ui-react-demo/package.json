{
  "name": "@accordproject/concerto-ui-react-demo",
  "version": "0.82.9",
  "description": "Form generator for Accord Project Concerto modeling language type",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "private": true,
  "dependencies": {
    "@accordproject/concerto-ui-react": "^0.82.9",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "^2.1.8",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.83.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "pretest": "npm run lint",
    "lint": "eslint .",
    "postlint": "npm run licchk",
    "licchk": "license-check-and-add",
    "postlicchk": "npm run doc",
    "doc": "jsdoc --pedantic --recurse -c jsdoc.json"
  },
  "keywords": [
    "blockchain",
    "hyperledger",
    "solutions"
  ],
  "author": "Hyperledger Composer",
  "license": "Apache-2.0",
  "devDependencies": {
    "jsdoc": "3.5.5",
    "license-check-and-add": "2.3.1"
  },
  "license-check-and-add-config": {
    "folder": ".",
    "license": "LICENSE.txt",
    "exact_paths_method": "EXCLUDE",
    "exact_paths": [
      "coverage",
      "LICENSE.txt",
      "node_modules",
      ".nyc-output",
      "out",
      ".git",
      "yarn.lock",
      "yarn-error.log",
      "README.md",
      "src/registerServiceWorker.js",
      "src/components/FormBuilder",
      "public",
      "config",
      ".babelrc",
      "dist"
    ],
    "file_type_method": "EXCLUDE",
    "file_types": [
      ".yml",
      ".yaml",
      ".zip",
      ".tgz"
    ],
    "insert_license": false,
    "license_formats": {
      "js|njk|pegjs|cto|acl|qry": {
        "prepend": "/*",
        "append": " */",
        "eachLine": {
          "prepend": " * "
        }
      },
      "npmrc|editorconfig|txt": {
        "eachLine": {
          "prepend": "# "
        }
      }
    }
  },
  "nyc": {
    "exclude": [
      "coverage/**",
      "lib/codegen/javascriptparser.js",
      "lib/codegen/parsejs.js",
      "lib/introspect/parser.js",
      "lib/acl/parser.js",
      "lib/query/parser.js",
      "out/**",
      "scripts/**",
      "systest/**",
      "test/**"
    ],
    "reporter": [
      "text-summary",
      "html",
      "cobertura",
      "lcov",
      "clover"
    ],
    "include": [
      "**/.js"
    ],
    "all": true,
    "check-coverage": true,
    "statements": 100,
    "branches": 100,
    "functions": 100,
    "lines": 100
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
