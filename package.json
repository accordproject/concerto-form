{
  "name": "@accordproject/concerto-ui",
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "lerna": "^3.20.2"
  },
  "scripts": {
    "repoclean": "lerna clean",
    "bootstrap": "lerna bootstrap",
    "test": "lerna run test",
    "build": "lerna exec -- npm run build",
    "demo:react": "cd packages/concerto-ui-react-demo && npm run start",
    "pkgcheck": "node ./scripts/pkgcheck.js",
    "pkgstamp": "node ./scripts/pkgstamp.js",
    "pkgbump": "node ./scripts/pkgbump.js && node ./scripts/pkgcheck.js --fix",
    "pkgset": "node ./scripts/pkgset.js",
    "depcheck": "node ./scripts/depcheck.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/accordproject/concerto-ui.git"
  },
  "keywords": [
    "blockchain",
    "hyperledger",
    "solutions"
  ],
  "author": "Matt Roberts",
  "license": "Apache-2.0",
  "dependencies": {
    "moment": "^2.24.0"
  },
  "version": "0.84.0"
}
